-- üîë SCRIPTKEY check
getgenv().SCRIPTKEY = getgenv().SCRIPTKEY or "ZORI-0"
local hour = os.date("!*t").hour
local validKey = "ZORI-" .. math.floor(hour / 2)

if getgenv().SCRIPTKEY ~= validKey then
    game.Players.LocalPlayer:Kick("‚ùå Invalid key. Check Discord for current key.")
    return
end

-- üß† Game Place ID Logic
local placeId = game.PlaceId

local scriptMap = {
    [3082002798] = "https://raw.githubusercontent.com/zerolamar/Obfuscate-Scripts/refs/heads/main/T-Titans%20Battlegrounds",      -- T-Titans Battlegrounds
    [5171347293] = "https://raw.githubusercontent.com/zerolamar/Obfuscate-Scripts/refs/heads/main/Cabin%20Crew%20Simulator -- Cabin Crew Simulator
}

-- üéØ Execute if recognized game
if scriptMap[placeId] then
    loadstring(game:HttpGet(scriptMap[placeId]))()
else
    game.Players.LocalPlayer:Kick("‚ö†Ô∏è Unsupported game. Script will not run here.")
end
