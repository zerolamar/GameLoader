-- üîë SCRIPTKEY check (2-hour rotation)
getgenv().SCRIPTKEY = getgenv().SCRIPTKEY or "ZORI-0"
local hour = os.date("!*t").hour
local validKey = "ZORI-" .. math.floor(hour / 2)

if getgenv().SCRIPTKEY ~= validKey then
    game.Players.LocalPlayer:Kick("‚ùå Invalid key. Check Discord for current key.")
    return
end

-- üß† Game Place ID Logi
local placeId = game.PlaceId

local scriptMap = {
    [3082002798] = "https://raw.githubusercontent.com/zerolamar/Obfuscate-Scripts/main/T-Titans%20Battlegrounds", -- T-Titans Battlegrounds
}

-- üß™ Attempt to fetch and run the game-specific script
local scriptURL = scriptMap[placeId]

if scriptURL then
    local success, result = pcall(function()
        return game:HttpGet(scriptURL)
    end)

    if success and result then
        local execSuccess, err = pcall(loadstring(result))
        if not execSuccess then
            warn("‚ö†Ô∏è Script execution error:", err)
            game.Players.LocalPlayer:Kick("‚ùå Script error: " .. tostring(err))
        end
    else
        warn("üö´ Failed to fetch script:", result)
        game.Players.LocalPlayer:Kick("‚ùå Could not load remote script.")
    end
else
    game.Players.LocalPlayer:Kick("‚ö†Ô∏è Unsupported game. Script will not run here.")
end
