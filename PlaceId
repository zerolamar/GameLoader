-- üîë Optional SCRIPTKEY check (comment to skip)
getgenv().SCRIPTKEY = getgenv().SCRIPTKEY or "ZORI-0"
local hour = os.date("!*t").hour
local validKey = "ZORI-" .. math.floor(hour / 2)

if getgenv().SCRIPTKEY ~= validKey then
    game.Players.LocalPlayer:Kick("‚ùå Invalid key. Check Discord for current key.")
    return
end

-- üß† Game Place ID Logic
local placeId = game.PlaceId

local scriptMap = {
    [3082002798] = "https://raw.githubusercontent.com/zerolamar/Obfuscate-Scripts/main/T-Titans%20Battlegrounds.lua",
}

if scriptMap[placeId] then
    local success, result = pcall(function()
        return game:HttpGet(scriptMap[placeId])
    end)

    if success then
        loadstring(result)()
    else
        warn("üö´ Failed to load script:", result)
        game.Players.LocalPlayer:Kick("‚ùå Error loading script. Check GitHub link.")
    end
else
    game.Players.LocalPlayer:Kick("‚ö†Ô∏è Unsupported game. Script will not run here.")
end
